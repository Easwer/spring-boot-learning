    <artifactId>spring-data-parent</artifactId>
    <version>2.3.1.RELEASE</version>
  </parent>
  <groupId>org.springframework.data</groupId>
  <artifactId>spring-data-r2dbc</artifactId>
  <version>1.1.1.RELEASE</version>
